<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_wecom_checkin_rule_tree" model="ir.ui.view">
            <field name="name">Wecom Checkin Rules List</field>
            <field name="model">wecom.checkin.rule</field>
            <field name="arch" type="xml">
                <tree edit="false" create="false">
                    <field name="name"/>
                    <field name="grouptype"/>
                    <field name="type"/>
                    <field name="sync_holidays"/>
                    <field name="need_photo"/>
                    <field name="note_can_use_local_pic"/>
                    <field name="allow_checkin_offworkday"/>
                    <field name="allow_apply_offworkday"/>
                    <field name="allow_apply_bk_cnt"/>
                    <field name="option_out_range"/>
                    <field name="use_face_detect"/>
                    <field name="allow_apply_bk_day_limit"/>
                    <field name="create_time"/>
                </tree>
            </field>
        </record>

        <record id="action_view_wecom_checkin_rule_list" model="ir.actions.act_window">
            <field name="name">Wecom Checkin Rules</field>
            <field name="res_model">wecom.checkin.rule</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="view_id" eval="False"/>
        </record>
    </data>
</odoo>